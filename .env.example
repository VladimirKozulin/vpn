# === КРИТИЧНЫЕ ПАРАМЕТРЫ (СЕКРЕТЫ) ===
# Reality приватный ключ - сгенерируй свой!
VPN_REALITY_PRIVATE_KEY=your_private_key_here

# Reality публичный ключ - сгенерируй свой!
VPN_REALITY_PUBLIC_KEY=your_public_key_here

# === КОНФИГУРАЦИЯ СЕРВЕРА ===
# IP адрес или домен вашего VPS
VPN_SERVER_ADDRESS=your_server_ip_or_domain

# === БАЗА ДАННЫХ VPN ===
DB_HOST=localhost
DB_PORT=5432
DB_NAME=vpn_db
DB_USERNAME=postgres
DB_PASSWORD=postgres

# === БАЗА ДАННЫХ KEYCLOAK ===
KEYCLOAK_DB_PORT=5433
KEYCLOAK_DB_NAME=keycloak_db
KEYCLOAK_DB_USERNAME=keycloak
KEYCLOAK_DB_PASSWORD=keycloak

# === KEYCLOAK СЕРВЕР ===
KEYCLOAK_PORT=8180
KEYCLOAK_URL=http://localhost:8180
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# === KEYCLOAK ИНТЕГРАЦИЯ ===
KEYCLOAK_REALM=vpn-realm
KEYCLOAK_CLIENT_ID=vpn-client
KEYCLOAK_CLIENT_SECRET=your_client_secret_here

# === XRAY API ===
VPN_API_SERVER=127.0.0.1:10085
VPN_INBOUND_TAG=inbound-443

# === ОПЦИОНАЛЬНЫЕ (можно оставить дефолты) ===
VPN_XRAY_PORT=443
VPN_XRAY_PATH=/opt/xray/xray
VPN_CONFIG_PATH=xray-config.json
VPN_REALITY_ENABLED=true
VPN_REALITY_DEST=www.microsoft.com:443
VPN_REALITY_FINGERPRINT=chrome
